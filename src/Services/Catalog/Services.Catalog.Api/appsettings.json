{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenApi": {
    "Endpoint": {
      "Name": "Catalog.API V1"
    },
    "Document": {
      "Description": "The Catalog Service API",
      "Title": "Food Express - Catalog API",
      "Version": "v1"
    },
    "Auth": {
      "ClientId": "catalog-openapi",
      "AppName": "Catalog OpenApi with Scalar"
    }
  },
  "Identity": {
    "Audience": "catalog-api",
    "Scopes": {
      "catalogAll": "Catalog scopes"
    }
  },
  "CorsOptions": {
    "AllowedOrigins": [
      "https://localhost:3000"
    ]
  }
}
